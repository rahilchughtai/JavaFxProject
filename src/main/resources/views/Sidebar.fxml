<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>


<VBox spacing="30"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controllers.SceneController">
    <Button onAction="#btnSwitchToHome"  mnemonicParsing="false" styleClass="btn-primary" text="Home">
        <graphic>
            <FontIcon styleClass="icon" iconLiteral="mdi-home" />
        </graphic>
    </Button>
    <Button onAction="#btnSwitchToRooms"  mnemonicParsing="false" styleClass="btn-primary" text="Rooms">
        <graphic>
            <FontIcon styleClass="icon" iconLiteral="mdi-table" />
        </graphic>
    </Button>
    <Button onAction="#btnSwitchToStudents" mnemonicParsing="false" styleClass="btn-primary" text="Students">
        <graphic>
            <FontIcon styleClass="icon" iconLiteral="fa-graduation-cap" />
        </graphic>
    </Button>
    <Button onAction="#btnSwitchToCourses" mnemonicParsing="false" styleClass="btn-primary" text="Courses">
        <graphic>
            <FontIcon styleClass="icon" iconLiteral="mdi-table" />
        </graphic>
    </Button>
</VBox>
