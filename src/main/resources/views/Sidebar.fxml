<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="30"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controllers.SceneController">
    <Button  onAction="#btnSwitchToHome"  mnemonicParsing="false" styleClass="btn-primary" text="Home">
    </Button>
    <Button onAction="#btnSwitchToRooms"  mnemonicParsing="false" styleClass="btn-primary" text="Rooms">
    </Button>
    <Button onAction="#btnSwitchToStudents" mnemonicParsing="false" styleClass="btn-primary" text="Students">
    </Button>
    <Button onAction="#btnSwitchToCourses" mnemonicParsing="false" styleClass="btn-primary" text="Courses">
    </Button>
</VBox>
